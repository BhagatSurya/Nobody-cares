<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%=pageTitle%></title>
    <style>
        /* Add your styling here */
        body {
            font-family: Arial, sans-serif;
            margin: 20px;
        }

        .video-grid {
            display: grid;
            grid-template-columns: repeat(auto-fill, minmax(300px, 1fr));
            gap: 20px;
        }

        .video-item {
            border: 1px solid #ddd;
            padding: 10px;
            text-align: center;
        }

        iframe {
            width: 100%;
            height: 200px; /* Adjust the height as needed */
        }
    </style>
</head>
<body>
    <h1>Video List</h1>
    <div class="video-grid">
        <% videoUrls.forEach(function(video) { %>
            <div class="video-item">
                <!-- Display YouTube video in an iframe -->
                <iframe src="<%= video.url %>" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
            </div>
        <% }); %>
    </div>
</body>
</html>
